    "# Example 3: NumPy - Boolean Indexing\n",
    "import numpy as np\n",
    "# Creating an array\n",
    "array_1d = np.array([10, 20, 30, 40, 50])\n",
    "\n",
    "# Boolean indexing\n",
    "greater_than_30 = array_1d[array_1d > 30]\n",
    "\n",
    "print('Elements Greater Than 20:', greater_than_30)"
